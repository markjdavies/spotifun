		"@vercel/node": "^2.5.7",
		"fp-ts": "^2.12.2",
		"ono": "^7.1.3",
		"pino": "^7.2.0",
		"spotify-web-api-node": "^5.0.2",
		"znv": "^0.3.1",
		"zod": "^3.18.0"
	},
	"devDependencies": {
